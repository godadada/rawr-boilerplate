{
  "name": "rawr-boilerplate",
  "version": "1.0.0",
  "description": "boilerplate to get started building a Redux Apollo Webpack React (on express) app",
  "scripts": {
    "build:client": "NODE_ENV=production webpack --config config/webpack.config.prod.js",
    "build:server": "NODE_ENV=production webpack --config config/webpack.config.prod-server.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "clean": "rimraf dist/client/bundle.* && rimraf dist/server/main.*",
    "start": "babel-watch server/dev.js",
    "tunnel": "browser-sync start --proxy localhost:3000 --tunnel rkstar",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rkstar/rawr-boilerplate.git"
  },
  "keywords": [
    "redux",
    "apollo",
    "apollostack",
    "webpack",
    "react",
    "express",
    "node",
    "graphql",
    "mongodb"
  ],
  "author": "David Fudge <rkstar@mac.com> (https://github.com/rkstar)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rkstar/rawr-boilerplate/issues"
  },
  "homepage": "https://github.com/rkstar/rawr-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-watch": "^2.0.2",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^5.2.2",
    "json-loader": "^0.5.4",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "postcss-nested": "^1.0.0",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.10",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.0",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "apollo-client": "^0.3.26",
    "apollo-server": "^0.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "express": "^4.14.0",
    "graphql-tag": "^0.1.7",
    "graphql-tools": "^0.6.0",
    "lodash": "^4.13.1",
    "meteor-random": "0.0.3",
    "moment": "^2.13.0",
    "moment-timezone": "^0.5.4",
    "mongoose": "^4.5.3",
    "react": "^15.2.0",
    "react-addons-css-transition-group": "^15.2.0",
    "react-apollo": "^0.3.13",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
